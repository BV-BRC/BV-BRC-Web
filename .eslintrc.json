{
  "extends": "airbnb-base/legacy",
  "rules": {
    "_comment_reactivate_later": ["no-useless-concat", "block-scoped-var", "no-restricted-globals", "no-redeclare",
      "no-nested-ternary", "no-return-assign", "no-restricted-syntax", "no-multi-assign", "no-loop-func",
      "array-callback-return", "no-use-before-define", "no-template-curly-in-string", "no-new", "no-cond-assign",
      "no-throw-literal", "no-dupe-keys", "no-extend-native",
      "no-plusplus", "no-unused-vars", "no-bitwise"],
    "no-useless-concat": 0,
    "block-scoped-var": 0,
    "no-restricted-globals": 0,
    "no-redeclare": 0,
    "no-nested-ternary": 0,
    "no-return-assign": 0,
    "no-restricted-syntax": 0,
    "no-multi-assign": 0,
    "no-loop-func": 0,
    "array-callback-return": 0,
    "no-use-before-define": 0,
    "no-template-curly-in-string": 0,
    "no-cond-assign": 0,
    "no-throw-literal": 0,
    "no-dupe-keys": 0,
    "no-extend-native": 0,
    "no-new": 0,
    "one-var": 0,
    "eqeqeq": 0,
    "camelcase": 0,
    "no-plusplus": [1, {"allowForLoopAfterthoughts": true}],
    "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
    "no-plusplus": 0,
    "no-unused-vars": 0,
    "no-lonely-if": 0,
    "no-prototype-builtins": 0,
    "no-bitwise": ["error", { "allow": ["~"] }],
    "no-bitwise": 0,
    "radix": [1, "as-needed"],
    "no-multi-spaces": 0,
    "consistent-return": 0,
    "no-unused-expressions": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "func-names": 0,
    "no-shadow": 0,
    "guard-for-in": 0,
    "space-in-parens": 0,
    "key-spacing": 0,
    "max-len": 0,
    "padded-blocks": 0,
    "no-console": 0,
    "no-continue": 0,
    "import/no-extraneous-dependencies": 0,
    "import/newline-after-import": 0,
    "no-mixed-operators": 0,
    "no-undef": 0,
    "prefer-template": 0,
    "no-debugger": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "default-case": 0,
    "new-cap": 0,
    "semi-spacing": 0,
    "brace-style": 0,
    "no-alert": 0,
    "allowKeywords": 0,
    "no-dupe-args": 0
  }
}
